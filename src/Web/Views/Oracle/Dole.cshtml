@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Name</title>
    <script src="~/Scripts/bundle.js"></script>
    <link rel="stylesheet" href="/css/font-awesome.min.css">
    <link rel="shortcut icon" href="~/favicon-tasks.ico" />
</head>
<body>
    <div id="entry"></div>
    <script>
    ReactDOM.render(
        React.createElement(Components.Dole, {
            year: "@ViewBag.Year",
            version: "@ViewBag.Version",
            containerId: "@ViewBag.ContainerId",
            doleId: "@ViewBag.DoleId"
        }),
        document.getElementById('entry')
    );
    </script>
</body>
</html>
